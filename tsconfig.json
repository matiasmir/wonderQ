{
    "compilerOptions": {
       "lib": [
          "es5",
          "es6"
       ],
       "target": "es5",
       "module": "commonjs",
       "resolveJsonModule": true,
       "moduleResolution": "node",
       "outDir": "./build",
       "emitDecoratorMetadata": true,
       "experimentalDecorators": true,
       "sourceMap": true             
    },
    "exclude": ["**/*.spec.ts"],
    "typeRoots": ["./src/utils/types", "./node_modules/@types"]
 }
